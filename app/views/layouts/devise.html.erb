<!DOCTYPE html>
<html>
  <head>
    <title>Stellar - Login Page</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body class="myClass">
    <nav class="navbar navbar-inverse navbar-fixed-top" style="height:9%; background-color: #1f232c">
      <div class="container-fluid" style="margin-top:5px;">
        <div class="row">
          <div class="col-sm-6 col-md-4 col-md-offset-4">
          <%= link_to image_tag("logo.png", class: 'img-responsive center-block', style: 'margin-top:3%;'), root_path %>
          </div>
        </div>  
      </div>
    </nav>

    <div class="container">
      <div class="row">
        <div class="col-md-6">
          
        </div>
        <div class="col-md-1"></div>
        <div class="col-md-5" style="margin-top: 30px;">
          <%= yield %>
        </div>
      </div>
    </div>      
  </body>
</html>